/* 四联图片的剪裁 */

/* 最小化按钮 */
QPushButton#buttonMin{
    min-width: 27px;
    min-height: 27px;
    max-width: 27px;
    max-height: 27px;
    border:0px;
    border-radius:1;

    /* 右侧 减掉 81px， 左侧剪去 0， 第一张图片 */
    border-image:url(:/assets/images/TitleQss/min.png) 0 81 0 0 ;

}

QPushButton#buttonMin:hover
{
    /* 右侧减去 54， 左侧剪去 27， 第二张图片 */
    border-image:url(:/assets/images/TitleQss/min.png) 0 54 0 27 ;
}

QPushButton#buttonMin:pressed
{
    /* 右侧减去 27, 左侧剪去 54， 第三张图片 */
    border-image:url(:/assets/images/TitleQss/min.png) 0 27 0 54 ;
}



/* 最大化按钮 */
QPushButton#buttonMax{
    min-width: 27px;
    min-height: 27px;
    max-width: 27px;
    max-height: 27px;
    border-image:url(:/assets/images/TitleQss/max.png) 0 81 0 0 ;
}

QPushButton#buttonMax:hover
{
    border-image:url(:/assets/images/TitleQss/max.png) 0 54 0 27 ;
}

QPushButton#buttonMax:pressed
{
    border-image:url(:/assets/images/TitleQss/max.png) 0 27 0 54 ;
}



/* 还原按钮 */
QPushButton#buttonRestore{
    min-width: 27px;
    min-height: 27px;
    max-width: 27px;
    max-height: 27px;
    border-image:url(:/assets/images/TitleQss/restore.png) 0 81 0 0 ;
   
}

QPushButton#buttonRestore:hover
{
    border-image:url(:/assets/images/TitleQss/restore.png) 0 54 0 27 ;
    
}

QPushButton#buttonRestore:pressed
{
    border-image:url(:/assets/images/TitleQss/restore.png) 0 27 0 54 ;
    
}


/* 关闭按钮 */
QPushButton#buttonClose{
    min-width: 27px;
    min-height: 27px;
    max-width: 27px;
    max-height: 27px;
    border-image:url(:/assets/images/TitleQss/close.png) 0 81 0 0 ;
    border-top-right-radius:3 ;
    border-top-left-radius:1px;
    border-bottom-left-radius:1px;
    border-bottom-right-radius:1px;
}

QPushButton#buttonClose:hover
{
    border-image:url(:/assets/images/TitleQss/close.png) 0 54 0 27 ;
    border-top-right-radius:3 ;
}

QPushButton#buttonClose:pressed
{
    border-image:url(:/assets/images/TitleQss/close.png) 0 27 0 54 ;
    border-top-right-radius:3 ;
}



QComboBox[editable="false"],
QComboBox[editable="true"]::drop-down {
    border-image: url(:/qss/pagefold/pushbutton.png) 5;
    border-width: 5;
}


QComboBox[editable="false"]:hover,
QComboBox[editable="true"]::drop-down:hover, QMenuBar::item:hover {
    border-image: url(:/qss/pagefold/pushbutton_hover.png) 5;
    border-width: 5;
}


QComboBox[editable="false"]:on,
QComboBox[editable="true"]::drop-down:on, QMenuBar::item:on {
    border-image: url(:/qss/pagefold/pushbutton_pressed.png) 5;
    border-width: 5;
}


