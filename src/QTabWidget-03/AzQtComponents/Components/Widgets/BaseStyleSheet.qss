/* ============================================================= */
/* Global styles                                                 */
/* ============================================================= */


*
{
    background-color: #444444;
    color: white;
    font-family: "Amazon Ember";
    font-weight: 400;
}

QMainWindow,
QDialog,
QDockWidget
{
    color: #cccccc;
    background-color: #393a3c;
}

QTextEdit,
QPlainText,
QLineEdit
{
    background-color: white;
    color: black;
    font-family: "Amazon Ember";
}


